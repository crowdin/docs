---
/**
    Custom page without the header to display it in the application in iframe
*/
import StarlightPage from '@astrojs/starlight/components/StarlightPage.astro';
import Article from '~/content/docs/docs/terms.mdx';
---

<StarlightPage frontmatter={{
    title: 'Terms and Conditions',
    template: 'splash',
    pagefind: false,
    hideHeader: true
}}>
    <Article />
</StarlightPage>
