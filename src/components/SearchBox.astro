---
import Default from '@astrojs/starlight/components/Search.astro';

const labels: {[key: string]: string} = {
    "search.label": "Search",
    "search.ctrlKey": "Ctrl",
    "search.cancelLabel": "Cancel",
    "search.devWarning": "Search is only available in production builds.",
};
---

<style>
    :root {
        site-search button > kbd kbd {
            margin-top: 0;
        }
    }
</style>

<Default labels={labels} {...Astro.props as any} />